    "@babel/preset-typescript": "^7.1.0",
    "babel-loader": "^8.0.4",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
